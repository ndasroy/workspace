
public class Inheritance 
{

	public static void main()
	{
		Car C=new Car();
		
	}
	
}	
	class vec
	{
		int wheel=4;
		void Display()
		{
			System.out.println("I am Display..");
		}
		
		
	}
	
	class Car extends vec
	{
		void Display()
		{
			System.out.println("How r u...?");
		}
	
		void Print()
		{
			System.out.println("I am Car...");
		}
		
	}
